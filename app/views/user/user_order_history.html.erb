

<h1> Your Orders </h1>
<div class="table-responsive">
<table class="table table-striped table-sm">
  <thead>
    <tr>
      <th>Oder_Id</th>
      <th>Product</th>
      <th>Item</th>
      <th>Qauntity</th>
      

      
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |o| %>
      <tr>
        <td><%= o.id %></td>
        
        <td><%= o.product.product_name %></td> 
        <td><%= o.item.item_name %></td> 
        <td><%= o.quantity %></td> 
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<div>
<%= link_to "Home", user_home_path %>
</div>
